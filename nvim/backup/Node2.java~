public class Node2 {
    private int data;
    private Node2 next; // Self-referencing data structure

    public Node2(int n) {
        data = n;
        next = null;
    }

    public void setData(int n) {
        data = n;
    }

    public int getData() {
        return data;
    }

    public void setNext(Node2 n) {
        next = n;
    }

    public Node2 getNext() {
        return next;
    }

    public String toString() {
        return "" + data;
    }
}

