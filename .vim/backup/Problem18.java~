public class Problem18 {
    public static void main(String[] args) {
        D(5);
    }

    public static int D(int n) {
        int[][] matrix = new int[n][n];
        for (int i = 0 ; i < n ; i++) {
            for (int j = 0 ; j < n ; j++) {
                matrix[i][j] = min(2 * i + 1, 2 * j + 1);
            }
        }

        for (int i = 0 ; i < n ; i++) {
            for (int j = 0 ; j < n ; j++) {
                System.out.print(matrix[i][j] + ' ');
            }
            System.out.print("\n");
        }

        return 1;
    }

    public static int min(int a, int b) {
        if (a <= b) {
            return a;
        }
        else {
            return b;
        }
    }
}
