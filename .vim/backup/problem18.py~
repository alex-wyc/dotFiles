def D(n):
    matrix = [[1] * n] * n
    for i in range(0, n):
        for j in range(0, n):
            print min(2 * 1 + 1, 2 * j + 1)
#            print "(%d, %d) => (%d, %d) --> %d" % (i, j, 2 * i + 1, 2 * j + 1,
                    min(2 * i + 1, 2 * j + 1))
            matrix[i][j] = min([2 * i + 1, 2 * j + 1])

    print matrix

D(3)
D(2)
D(1)
