myLength :: [a] -> Int
myLength [] = 0
myLength [x] = 1
myLength (x:xs) = 1 + myLength xs

myLast :: [a] -> a
myLast [] = error "No last el on empty list"
myLast [x] = x
myLast (x:xs) = myLast xs

myButLast :: [a] -> a
myButLast [] = error "No second to last on empty list"
myButLast [x] = error "No second to last on a list of size 1"
myButLast [x:y] = x
myButLast (x:xs) = myButLast xs
